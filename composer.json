{
  "name": "utf8-speed-test",
  "description": "A benchmark for UTF-8 libs",
  "version": "0.0.1",
  "config": {
    "optimize-autoloader": true,
    "vendor-dir": "vendor/composer/",
    "github-protocols": [
      "git",
      "https",
      "http"
    ]
  },
  "require": {
    "voku/portable-utf8": "dev-master",
    "patchwork/utf8": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
